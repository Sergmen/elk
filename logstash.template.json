{
    "template": "logstash",
    "index_patterns": [
        "logstash-*"
    ],
    "settings": {
        "number_of_shards": 1
    },
    "mappings": {
        "properties": {
            "message": {
                "type": "keyword"
            },
            "source": {
                "type": "keyword"
            },
            "level": {
                "type": "keyword"
            },
            "time": {
                "type": "keyword"
            }
        }
    }
}